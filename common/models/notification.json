{
  "name": "Notification",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string",
      "required": true
    }
  },
  "relations": {
    "hiwuUser": {
      "type": "belongsTo",
      "model": "HiwuUser",
      "foreignKey": "userId"
    },
    "fromUser": {
      "type": "belongsTo",
      "model": "HiwuUser",
      "foreignKey": "fromId"
    }
  }
}
